<fieldset>
<legend>
  <h3>Restaurant Directory</h3>
</legend>
  <ol>
    <% @restaurants.each do |restaurant| %>
      <li>
        <%= link_to restaurant.name, restaurant %>
      </li>
    <% end %>
  </ol>
</fieldset>
</br>
<%= link_to "Back", root_path %>
<% if logged_in? %>
  <%= link_to "User Page", user_path(current_user) %>
<% end %>
<%= link_to "See Reviews", reviews_path %>
<%= link_to "Top Reviews", reviews_path(rating: "Excellent") %>
